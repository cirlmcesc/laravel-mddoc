{
    "name": "cirlmcesc/laravel-mddoc",
    "description": "read markdown file content rendering web page",
    "license": "MIT",
    "type": "laravel-package",
    "keywords": [
        "laravel",
        "documentation",
        "md",
        "api-doc"
    ],
    "authors": [
        {
            "name": "Cirlmcesc Ma",
            "email": "cirlmcesc_ma@163.com"
        }
    ],
    "homepage": "https://github.com/cirlmcesc/laravel-mddoc",
    "minimum-stability": "stable",
    "require": {
        "php": "^7.2.5",
        "illuminate/support": "^7.0",
        "illuminate/console": "^7.0",
        "erusev/parsedown": "^1.6"
    },
    "require-dev": {
        "orchestra/testbench": "~3.6"
    },
    "autoload": {
        "classmap": [
            "src/Console"
        ],
        "psr-4": {
            "Cirlmcesc\\LaravelMddoc\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "Cirlmcesc\\LaravelMddoc\\LaravelMddocServiceProvider"
            ]
        }
    }
}